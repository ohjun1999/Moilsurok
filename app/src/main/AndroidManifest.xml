<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.moilsurok">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Moilsurok">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DateActivity"/>
        <activity android:name=".ExtraActivity"/>
        <activity android:name=".AlarmActivity"/>
        <activity android:name=".DateDetailActivity"/>
        <activity android:name=".ExplainGroupActivity"/>
        <activity android:name=".NoteActivity"/>
        <activity android:name=".NoteProfileChangeActivity"/>
        <activity android:name=".NoticeActivity"/>
        <activity android:name=".ProfileModifyActivity"/>
        <activity android:name=".ManageAuthenticationMethodsActivity"/>
        <activity android:name=".InquiryActivity"/>
        <activity android:name=".AssociationInquiryActivity"/>
        <activity android:name=".TermsActivity"/>
    </application>

</manifest>